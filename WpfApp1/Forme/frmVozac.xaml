            <Label Content="Kontakt" HorizontalAlignment="Left" Margin="143,153,0,0" VerticalAlignment="Top"  FontSize="12" FontFamily="Microsoft JhengHei UI" Width="75" RenderTransformOrigin="0.513,-0.32"/>
            <TextBox Name="txtKontaktVozac" HorizontalAlignment="Left" Height="23" Margin="223,153,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" BorderBrush="#FF0D0D0E" CaretBrush="#FF04000A" RenderTransformOrigin="1.696,0.5"/>
            <Label Content="Adresa" HorizontalAlignment="Left" Margin="143,183,0,0" VerticalAlignment="Top" FontSize="12" FontFamily="Microsoft JhengHei UI" Width="75" RenderTransformOrigin="0.607,0.14"/>
